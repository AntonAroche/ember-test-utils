{
  "name": "ember-test-utils",
  "version": "1.6.0",
  "description": "Utilities to assist with testing ember apps/addons",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "npm run lint && ember test",
    "ci-test": "ember try:one $EMBER_TRY_SCENARIO --- COVERAGE=true ember test",
    "lint": "eslint *.js addon app config test-support tests"
  },
  "repository": "git@github.com:ciena-blueplanet/ember-test-utils.git",
  "engines": {
    "node": ">= 5.0.0"
  },
  "author": "Sophy Pal (https://github.com/sophypal)",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.5.0",
    "ember-cli": "^2.10.1",
    "ember-cli-app-version": "^2.0.1",
    "ember-cli-chai": "0.3.2",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-htmlbars": "^1.1.1",
    "ember-cli-htmlbars-inline-precompile": "0.3.6",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mocha": "0.13.1",
    "ember-cli-test-loader": "^1.1.1",
    "ember-cli-uglify": "^1.2.0",
    "ember-computed-decorators": "0.3.0",
    "ember-data": "^2.11.0",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-export-application-global": "^1.1.1",
    "ember-inflector": "^1.11.0",
    "ember-load-initializers": "0.6.3",
    "ember-lodash-shim": "^1.1.0",
    "ember-resolver": "^2.1.1",
    "ember-sinon": "0.6.0",
    "eslint": "^3.13.1",
    "eslint-config-frost-standard": "^5.3.2",
    "loader.js": "^4.1.0",
    "lodash-es": "^4.17.4"
  },
  "keywords": [
    "ember-addon"
  ],
  "dependencies": {
    "ember-cli-babel": "^5.1.5"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}